<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yossra.EmailController">
   <children>
      <Label layoutX="51.0" layoutY="58.0" text="To:" textFill="DODGERBLUE">
         <font>
            <Font name="Arial Rounded MT Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="28.0" layoutY="126.0" text="Subject:" textFill="DODGERBLUE">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="19.0" layoutY="188.0" text="Message:" textFill="DODGERBLUE">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="tfsend" layoutX="428.0" layoutY="77.0" mnemonicParsing="false" onAction="#sendemail" style="-fx-background-color: #1E90FF; -fx-background-radius: 20;" text="SendEmail" textFill="WHITE" />
      <Button fx:id="tfretour" layoutX="428.0" layoutY="148.0" mnemonicParsing="false" onAction="#retour" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #1E90FF; -fx-background-radius: 20;" text="Retour" textFill="WHITE" />
      <TextField fx:id="tfto" layoutX="122.0" layoutY="60.0" promptText="To" />
      <TextField fx:id="tfsub" layoutX="121.0" layoutY="126.0" promptText="Subject" />
      <TextArea fx:id="tfmesg" layoutX="122.0" layoutY="173.0" prefHeight="200.0" prefWidth="200.0" promptText="Message" />
   </children>
</AnchorPane>
